  21          // Check struct increased by interrupt
  22          //---------------------------------------------------------------------------
  23          u8 inc_check (STRUCT_INT_CNT *p)
  24          {
  25   1          u8 cur;
  26   1          cur = p->cnt;
  27   1          if (cur > p->bak)       // normal increase
  28   1          {
  29   2              cur = cur - p->bak;
  30   2              p->bak = p->cnt;
  31   2          }
  32   1          else if (cur < p->bak)  // overflow increase
  33   1          {
  34   2              cur = 256 - p->bak + cur;
  35   2              p->bak = p->cnt;
  36   2          }
  37   1          else                    // no increase
  38   1          {
  39   2              cur = 0;
  40   2          }
  41   1          return cur;
  42   1      }
